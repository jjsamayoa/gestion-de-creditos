                <group name="studio_group_omBsC_right" string="Recibos emitidos">
                  <field filename="x_studio_recibos_filename" name="x_studio_recibos" string="Recibos"/>
                  <field invisible="1" name="x_studio_recibos_filename"/>
                </group>
              </group>
            </page>
            <page name="studio_page_pXWY8" string="Varios">
              <group name="studio_group_pXWY8">
                <group name="studio_group_pXWY8_left" string="Tipo de cliente">
                  <field name="x_studio_tipo_de_cliente"/>
                </group>
                <group name="studio_group_pXWY8_right" string="Documentos Escaneados">
                  <field name="x_studio_hoja_de_solicitud" string="Hoja de Solicitud"/>
                  <field filename="x_studio_dpi_filename" name="x_studio_dpi" string="Dpi"/>
                  <field invisible="1" name="x_studio_recibos_de_agua_y_luz_filename"/>
                  <field filename="x_studio_recibos_de_agua_y_luz_filename" name="x_studio_recibos_de_agua_y_luz" string="Recibos de Agua y Luz"/>
                  <field invisible="1" name="x_studio_cartas_de_recomendacin_filename"/>
                  <field filename="x_studio_cartas_de_recomendacin_filename" name="x_studio_cartas_de_recomendacin" string="Cartas de Recomendación"/>
                  <field invisible="1" name="x_studio_dpi_filename"/>
                </group>
              </group>
            </page>
            <page name="studio_page_UVt5K" string="Resumen de pagos">
              <group name="studio_group_UVt5K">
                <group name="studio_group_UVt5K_left"/>
                <group name="studio_group_UVt5K_right"/>
              </group>
            </page>
          </notebook>
        </xpath>
        <xpath expr="//form[1]/sheet[1]" position="after">
          <div class="oe_chatter" name="studio_div_0461b9">
